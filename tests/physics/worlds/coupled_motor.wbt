DEF CHECK_COUPLING Robot {
  children[
    HingeJoint {
      device [
        RotationalMotor {
          name "motor group a"
        }
      ]
    }
    HingeJoint {
      device [
        RotationalMotor {
          name "motor group a::subgroup b"
        }
      ]
    }
    HingeJoint {
      device [
        RotationalMotor {
          name "motor group a::subgroup c"
        }
      ]
    }
    HingeJoint {
      device [
        RotationalMotor {
          name "motor group b::subgroup b"
        }
      ]
    }
  ]
  name "coupling test"
  controller "coupled_motor"
  controllerArgs "coupling_test"
  supervisor TRUE
}
