# The Background node defines the background used for rendering the 3D world.

Background {
  vrmlField MFColor  skyColor [ 0 0 0 ]

  vrmlField MFString backUrl   []
  vrmlField MFString bottomUrl []
  vrmlField MFString frontUrl  []
  vrmlField MFString leftUrl   []
  vrmlField MFString rightUrl  []
  vrmlField MFString topUrl    []

  field MFString backIrradianceUrl   []
  field MFString bottomIrradianceUrl []
  field MFString frontIrradianceUrl  []
  field MFString leftIrradianceUrl   []
  field MFString rightIrradianceUrl  []
  field MFString topIrradianceUrl    []

  field     SFFloat  luminosity  1
}
