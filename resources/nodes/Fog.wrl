# The Fog node provides a way to simulate atmospheric effects by blending objects with
# the color specified by the color field based on the distances of the various objects from the camera.

Fog {
  w3dField SFColor                                           color           1 1 1
  w3dField SFString{"LINEAR", "EXPONENTIAL", "EXPONENTIAL2"} fogType         "LINEAR"
  w3dField SFFloat                                           visibilityRange 0
}
