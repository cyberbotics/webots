# The IndexedFaceSet node represents a 3D shape formed by constructing faces (polygons) from vertices listed in the coord field.

IndexedFaceSet { 
  vrmlField SFNode  coord         NULL
  vrmlField SFNode  texCoord      NULL
  vrmlField SFBool  solid         TRUE      # ignored (assumed TRUE)
  vrmlField SFBool  ccw           TRUE
  vrmlField SFBool  convex        TRUE
  vrmlField MFInt32 coordIndex    []        # [-1,)
  vrmlField MFInt32 texCoordIndex []        # [-1,)
  vrmlField SFFloat creaseAngle   0
}
